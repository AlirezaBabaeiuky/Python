a = int(input("a = "))
b = int(input("b = "))
division_result = a / b
modulus_result = a % b
print("a / b =", division_result)
print("a % b =", modulus_result)

# note that input assigns values as strings not numbers use int(input()) to overcome this 
import numpy as np
print("Try programiz.pro")
arr = np.array([1, 3, 5, 7, 9])
print(arr)
arrdel = np.delete(arr, 2)
print(arrdel)
# is using Numpy array then use np.delete if regular python list which is slower and takes more memoery then do del or clear()




