a = [1, 6, 3, 7, 4, 5]
print(a)
n = len(a)
print(n)
found = False
for i in range(0, n) : 
    if a[i] % 2 == 0 :
        print(a[i]) 
        found = True
        if not found : 
            print("OOPS")
